{
  "name": "fitfab-ui",
  "version": "2.1.1",
  "description": "fitfab UI elements",
  "sideEffects": false,
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "prebuild": "rm -rf lib",
    "dev": "npm run storybook | rollup -c --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:css": "stylelint 'src/**/*.{js,ts,tsx}' ",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fitfab/fitfab-ui.git"
  },
  "keywords": [],
  "author": "Miguel Julio <migueljulio.developer.com> (http://fitfab.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fitfab/fitfab-ui/issues"
  },
  "homepage": "https://github.com/fitfab/fitfab-ui#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "6.3.0",
    "@storybook/addon-links": "6.0.19",
    "@storybook/addons": "6.0.19",
    "@storybook/react": "^6.2.9",
    "@testing-library/react": "8.0.6",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.5",
    "@types/styled-components": "4.4.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/eslint-plugin-tslint": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.6",
    "chromatic": "5.9.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-import": "2.24.0",
    "eslint-plugin-jsdoc": "30.7.13",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "4.3.8",
    "jest": "^24.9.0",
    "jest-dom": "3.5.0",
    "lint-staged": "10.5.4",
    "prettier": "1.19.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rollup": "1.32.1",
    "rollup-plugin-analyzer": "3.3.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "standard-version": "9.3.1",
    "styled-components": "4.4.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "26.5.6",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.3"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint",
      "npm run lint:css"
    ]
  }
}
