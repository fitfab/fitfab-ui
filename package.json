{
  "name": "fitfab-ui",
  "version": "1.0.1",
  "description": "fitfab UI elements",
  "main": "./lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "rm -rf lib",
    "dev": "npm run storybook | tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fitfab/fitfab-ui.git"
  },
  "keywords": [],
  "author": "Miguel Julio <miguelajulio@gmail.com> (http://fitfab.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fitfab/fitfab-ui/issues"
  },
  "homepage": "https://github.com/fitfab/fitfab-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-knobs": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addons": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "@types/jest": "^24.0.9",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.12",
    "babel-loader": "^8.0.5",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "standard-version": "^5.0.1",
    "styled-components": "^4.1.3",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "3.5.1"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "dependencies": {}
}
