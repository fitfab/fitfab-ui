{
  "name": "fitfab-ui",
  "version": "1.0.1",
  "description": "fitfab UI elements",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build": "npm run build:types && npm run build:js",
    "prebuild": "rm -rf lib",
    "dev": "npm run storybook | rollup -c --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fitfab/fitfab-ui.git"
  },
  "keywords": [],
  "author": "Miguel Julio <miguelajulio@gmail.com> (http://fitfab.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fitfab/fitfab-ui/issues"
  },
  "homepage": "https://github.com/fitfab/fitfab-ui#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-links": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@testing-library/react": "8.0.4",
    "@types/jest": "24.0.15",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/styled-components": "4.1.16",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.2",
    "jest": "24.8.0",
    "jest-dom": "3.5.0",
    "prettier": "1.18.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rollup": "1.16.2",
    "rollup-plugin-analyzer": "3.0.1",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-node-resolve": "5.1.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "standard-version": "6.0.1",
    "styled-components": "4.3.2",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.2"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.1.3"
  },
  "dependencies": {}
}
